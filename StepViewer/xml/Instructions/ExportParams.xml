<?xml version="1.0" encoding="UTF-8"?>
<cadsofttools version="2">
  <!-- Saves using XML export parametrs -->

  <!-- Saves as a raster image-->      
  <save>
      <ExportParams FileName="C:\Users\sampi\OneDrive\Documents\ABViewer 14\Drawings\ImageJPG" Format=".jpg">
         <Width>1024</Width>
         <Height>-1</Height>
         <Proportional>True</Proportional>
         <BitPerPixel>24</BitPerPixel>
         <!-- Set quality in percent for JPG -->
         <Quality>100</Quality>
         <MeasureInPixels>True</MeasureInPixels>
         <!-- Set transparent for GIF -->
         <Transparent>False</Transparent>
         <!-- Set dpi for JPG: always 2 parameters -->        
         <DPUX>96</DPUX>
         <DPUY>96</DPUY>
        <!-- Compression for TIFF: LZW, Deflate, JPEG, CCITT3, CCITT4, CCITT6, Rle, Auto, None -->
        <Compression>LZW</Compression>
      </ExportParams>
  </save>

  <!-- Saves as PDF-->
  <save>
      <ExportParams FileName="C:\Users\sampi\OneDrive\Documents\ABViewer 14\Drawings\ImagePDF" Format=".pdf">
          <PageWidth>1024</PageWidth><!-- page width in millimeters -->
          <PageHeight>1024</PageHeight><!-- page height in millimeters -->
          <Margin>5</Margin><!-- page margins in millimeters -->
          <Author>Author</Author>
          <DrawMode>1</DrawMode><!-- 0: Color; 1: Black&White; -->
          <BackgroundColor>$000000</BackgroundColor>
          <DefaultColor>$0000FF</DefaultColor>
          <Quality>0.5</Quality><!-- Value in range 0.5-0.01;  0.5: lowest quality; 0.01: highest quality; -->
          <!-- Select layouts for export: 0: Model; 1:AllPaperSpaces; 2:LayoutByName; 3:AllLayouts+Model; 4:CurrentLayout;  -->
          <LayoutExportMode>2</LayoutExportMode>
          <!-- Used if layouts export mode is 'LayoutByName' -->
          <LayoutNameExportMode>Model</LayoutNameExportMode>
      </ExportParams>
  </save>
                                     
  <!-- Saves as PNG -->
  <save>
      <ExportParams FileName="C:\Users\sampi\OneDrive\Documents\ABViewer 14\Drawings\ImagePNG" Format=".png">
         <Width>1024</Width>
         <Height>-1</Height>
         <Proportional>True</Proportional>
         <BitPerPixel>24</BitPerPixel>
         <MeasureInPixels>True</MeasureInPixels>
      </ExportParams>
  </save>

  <!-- When saving to DXF and DWG, the format version is inidicated in the Version parameter in the following way:
       AC1015 = AutoCAD2000;
       AC1018 = AutoCAD2004;
       AC1021 = AutoCAD2007; -->

  <!-- Saves as DXF -->
  <save>
      <ExportParams FileName="C:\Users\sampi\OneDrive\Documents\ABViewer 14\Drawings\ImageDXF" Format=".dxf">
        <Version>AutoCAD2004</Version>
        <IsConvertImageToOLE>True</IsConvertImageToOLE>
      </ExportParams>
  </save>

  <!-- Saves as DWG -->
  <save>
      <ExportParams FileName="C:\Users\sampi\OneDrive\Documents\ABViewer 14\Drawings\ImageDWG" Format=".dwg">
        <Version>AutoCAD2000</Version>
        <IsConvertImageToOLE>True</IsConvertImageToOLE>
      </ExportParams>
  </save>

  <!-- Saves as GCode -->
  <save>
      <ExportParams FileName="C:\Users\sampi\OneDrive\Documents\ABViewer 14\Drawings\ImageGCode" Format=".nc">
         <Precision>2</Precision>
         <FeedOnZ>2000</FeedOnZ>
         <FeedOnXY>100</FeedOnXY>
         <SpindleSpeed>2000</SpindleSpeed>
         <DepthOnZ>-2</DepthOnZ>
         <DepartureOnZ>5</DepartureOnZ>
         <!--MachineType:
             0: milling machine;
             1: cutting machine; -->
         <MachineType>0</MachineType>
         <!--WorkpieceZeroType:
             0: origin;
             1: top left;
             2: top right;
             3: bottom left;
             4: bottom right; -->
         <WorkpieceZeroType>0</WorkpieceZeroType>
         <WorkpieceZeroOffset>50,10</WorkpieceZeroOffset>
         <!-- Laser command and Delay only for 1: cutting machine -->
         <LaserOnCommand>M3</LaserOnCommand>
         <LaserOffCommand>M5</LaserOffCommand>
         <Delay>500</Delay>
      </ExportParams>
  </save>

</cadsofttools>


